//Overload

package overloading;

public class Overload 
{

    static Overload ob;
    void add(int a,int b)
    {
        System.out.println(a+b);
    }
    
    void add(double a,double b)
    {
        System.out.println(a+b);
    }
    
        void add(int a,int b,int c)
    {
        System.out.println(a+b+c);
    }
        
    void add()
    {
        System.out.println("No add here");
    }
}

//overload test


package overloading;

import static overloading.Overload.ob;

public class Overloading {

    
    public static void main(String[] args) 
    {
        Overload.ob = new Overload();
        ob.add();
        ob.add(5,10);
        ob.add(4.4,4.5);
        ob.add(5, 4, 10);
    }
    
}
//Overload

package overloading;

public class Overload 
{

    static Overload ob;
    void add(int a,int b)
    {
        System.out.println(a+b);
    }
    
    void add(double a,double b)
    {
        System.out.println(a+b);
    }
    
        void add(int a,int b,int c)
    {
        System.out.println(a+b+c);
    }
        
    void add()
    {
        System.out.println("No add here");
    }
}

//overload test


package overloading;

import static overloading.Overload.ob;

public class Overloading {

    
    public static void main(String[] args) 
    {
        Overload.ob = new Overload();
        ob.add();
        ob.add(5,10);
        ob.add(4.4,4.5);
        ob.add(5, 4, 10);
    }
    
}
